    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />

    <style>
        body {
            margin: 0;
            overflow-x: hidden;
        }

        .galeria-section {
            position: relative;
            min-height: 100vh;
            color: #fff;
            padding: 100px 0;
        }

        .background-blur {
            position: absolute;
            inset: 0;
            background: url('@Url.Content("~/Images/fiesta-belen/Foto4Belen.jpg")') no-repeat center center / cover;
            filter: blur(14px) brightness(0.6);
            z-index: 1;
        }

        .contenido {
            position: relative;
            z-index: 2;
        }



        /* 🖼 FOTOS */
        .foto-card {
            overflow: hidden;
            border-radius: 18px;
            box-shadow: 0 15px 35px rgba(0,0,0,0.4);
        }

            .foto-card img {
                width: 100%;
                height: 100%;
                object-fit: cover;
                transition: transform .4s ease;
            }

            .foto-card:hover img {
                transform: scale(1.08);
            }

        .hidden {
            display: none;
        }


        .btn-volver {
            border-radius: 30px;
            padding: 12px 30px;
            font-weight: 600;
        }

        /* 📸 CARPETAS CON FOTO */
        .carpeta-foto {
            position: relative;
            height: 520px; /* MISMO ALTO QUE LA IMAGEN */
            border-radius: 22px;
            overflow: hidden;
            background-size: cover;
            background-position: center;
            cursor: pointer;
            box-shadow: 0 25px 60px rgba(0,0,0,.6);
            transition: transform .4s ease, box-shadow .4s ease;
        }

            .carpeta-foto:hover {
                transform: scale(1.02);
                box-shadow: 0 35px 80px rgba(0,0,0,.8);
            }

        /* Capa oscura + texto */
        .carpeta-overlay {
            position: absolute;
            inset: 0;
            background: linear-gradient(to top, rgba(0,0,0,.65), rgba(0,0,0,.15));
            display: flex;
            flex-direction: column;
            justify-content: flex-end;
            align-items: center;
            padding-bottom: 40px;
            text-align: center;
        }

            .carpeta-overlay h3 {
                font-size: 2rem;
                letter-spacing: 2px;
                margin-bottom: 8px;
            }

            .carpeta-overlay span {
                font-size: .9rem;
                letter-spacing: 3px;
                opacity: .85;
            }


    @* EJEMPLO DE MODIFICACION DE FOTO CHICA PARA QUE SE ADAPTE CON LAS QUE TIENE ALADO *@
         .foto-destacada {
        max-height: 720px;
        margin-top: 10px; /* 👈 esto hizo que la foto 5 baje un poco para que no quede flotando la baja */
        }

        .foto-destacada img {
            height: 100%;
            object-fit: cover;
        }

    /* ----------------------------------------------------------------------------------- */
    .titulo-galeria {
    display: inline-block;
    padding: 18px 45px;
    border-radius: 60px;
    background: rgba(0, 0, 0, 0.55);
    backdrop-filter: blur(6px);
    box-shadow: 0 15px 40px rgba(0,0,0,.6);
    color: #fff;
    letter-spacing: 4px;
}

    </style>
</head>

