    <script>
        const fondo = document.getElementById("background");

        const fondoGeneral   = "url('@Url.Content("~/images/fiesta-belen/Foto4Belen.jpg")')";
        const fondoViviana   = "url('@Url.Content("~/images/fiesta-viviana/Foto5Viviana.jpg")')";
        const fondoVictoria  = "url('@Url.Content("~/images/fiesta-victoria/Foto12Victoria.jpg")')";
        const fondoAngela    = "url('@Url.Content("~/images/fiesta-angela/Foto1Angela.jpg")')";
        const fondoAngelina  = "url('@Url.Content("~/images/fiesta-angelina/Foto2Angelina.jpg")')";
        const fondoAntonellaH= "url('@Url.Content("~/images/fiesta-antonellaH/Foto7AntonellaH.jpg")')";
        const fondoBelen     = "url('@Url.Content("~/images/fiesta-belen/Foto3Belen.jpg")')";
        const fondoMaia      = "url('@Url.Content("~/images/fiesta-maia/Foto1Maia.jpg")')";
        const fondoPili      = "url('@Url.Content("~/images/fiesta-pili/Foto1Pili.jpg")')";

        function abrirCarpeta(nombre) {
            document.getElementById('carpetas').classList.add('hidden');
            document.getElementById('galeria-' + nombre).classList.remove('hidden');

            if (nombre === 'Viviana') fondo.style.backgroundImage = fondoViviana;
            if (nombre === 'Victoria') fondo.style.backgroundImage = fondoVictoria;
            if (nombre === 'Angela') fondo.style.backgroundImage = fondoAngela;
            if (nombre === 'Angelina') fondo.style.backgroundImage = fondoAngelina;
            if (nombre === 'AntonellaH') fondo.style.backgroundImage = fondoAntonellaH;
            if (nombre === 'Belen') fondo.style.backgroundImage = fondoBelen;
            if (nombre === 'Maia') fondo.style.backgroundImage = fondoMaia;
            if (nombre === 'Pili') fondo.style.backgroundImage = fondoPili;
        }

        function cerrarCarpeta() {
            document.getElementById('carpetas').classList.remove('hidden');
            document.querySelectorAll('[id^="galeria-"]').forEach(g => g.classList.add('hidden'));
            fondo.style.backgroundImage = fondoGeneral;
        }
    </script>

